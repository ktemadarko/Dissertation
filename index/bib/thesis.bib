@article{janghorban2014,
  title = {Skype Interviewing: {{The}} New Generation of Online Synchronous Interview in Qualitative Research},
  author = {Janghorban, Roksana and Roudsari, Robab Latifnejad and Taghipour, Ali},
  year = {2014},
