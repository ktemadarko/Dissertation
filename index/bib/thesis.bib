@article{hellstrand2020a,
  title = {All-Time Low Period Fertility in {{Finland}}: {{Demographic}} Drivers, Tempo Effects, and Cohort Implications},
  author = {Hellstrand, Julia and Nis{\'e}n, Jessica and Myrskyl{\"a}, Mikko},
  year = {2020},
  publisher = {{Routledge}},
  doi = {10.1080/00324728.2020.1750677},
  abstract = {The ongoing period fertility decline in the Nordic countries is particularly strong in Finland, where the total fertility rate (TFR) reached an all-time low of 1.41 in 2018. We analyse the decrease in Finland's TFR in 2010\textendash 17, and assess its consequences for cohort fertility using complementary approaches. Decomposition of this fertility decline shows that first births and women aged {$<$}30 are making the largest contributions. However, women aged 30\textendash 39 are also, for the first time in decades, experiencing a sustained fertility decline. Tempo adjustments to the TFR suggest that quantum change is part of the decline. Several forecasting methods indicate that cohort fertility is likely to decline from the long-lasting level of 1.85\textendash 1.95 to 1.75 or lower among women born in the mid-1980s. Without an exceptionally strong recovery in fertility, Finnish cohort fertility is likely to decline to levels currently observed among countries with very low fertility.},
  journal = {Population Studies},
  keywords = {cohort fertility,fertility postponement,Finland,forecasting,non-parametric approach,Nordic fertility regime,period TFR}
}

