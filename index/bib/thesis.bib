@article{tarzia2013,
  title = {Interviewing Older People in Residential Aged Care about Sexuality: {{Difficulties}} and Challenges},
  author = {Tarzia, Laura and Bauer, Michael and Fetherstonhaugh, Deirdre and Nay, Rhonda},
  year = {2013},
