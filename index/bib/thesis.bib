@article{goldstein2013,
  title = {Fertility {{Reactions}} to the "{{Great Recession}}" in {{Europe}}: {{Recent Evidence}} from {{Order}}-{{Specific Data}}},
  author = {Goldstein, Joshua and Karaman {\"O}rsal, Deniz Dilan and Kreyenfeld, Michaela and Jasilioniene, Aiva},
