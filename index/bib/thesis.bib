@article{butler2004,
  title = {Gay, Lesbian, Bisexual, and Transgender ({{GLBT}}) Elders: {{The}} Challenges and Resilience of This Marginalized Group},
  author = {Butler, Sandra S.},
  year = {2004},
  volume = {9},
