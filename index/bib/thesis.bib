@article{prior2008,
  title = {Repositioning Documents in Social Research},
  author = {Prior, Lindsay},
  year = {2008},
  volume = {42},
  pages = {821--836},
  doi = {10.1177/0038038508094564},
  abstract = {In matters of social research sociologists and other social scientists have tended to view documents primarily as sources of evidence and as receptacles of inert content.The key strategies for data exploration have consequently been associated with various styles of content or thematic analysis. Even when discourse analysis has been recommended, there has been a marked tendency to deal with records, files, and the like, primarily as containers - things to be read, understood, and categorized. In this article, however, the author seeks to demonstrate that by focussing on the functioning of documents instead of content, sociology can embrace a much wider range of approaches to both data collection and analysis. Indeed, the adoption of such a programme encourages researchers to see documents as active agents in the world, and to view documentation as a key component of dynamic networks rather than as a set of static and immutable 'things'. Copyright \textcopyright{} 2008 BSA Publications Ltd\textregistered.},
  journal = {Sociology},
